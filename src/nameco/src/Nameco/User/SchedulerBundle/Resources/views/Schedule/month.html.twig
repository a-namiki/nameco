{% extends 'NamecoUserSchedulerBundle:Base:calendar_month.html.twig' %}

{% block navigation %}
    {% include 'NamecoUserSchedulerBundle:Base:navigation.html.twig' with {'routeName': 'schedule_month_id_ym', 'selectRouteName':'schedule_selected_month'} %}
{% endblock %}

{% block submenu %}
    {% render "NamecoUserSchedulerBundle:SubMenu:scheduleMonth" with {'year': year, 'month': month} %}
{% endblock %}

{% block calendar %}
    {{ parent() }}
    <span id="schedule_new_prefix" style="display:none;">{{ path('schedule_new_prefix', {'userId': userId }) }}</span>
{% endblock %}