{% extends 'NamecoUserSchedulerBundle:Base:calendar_month.html.twig' %}

{% block navigation %}
    {% include 'NamecoUserSchedulerBundle:Base:navigation.html.twig' with {'routeName': 'establishment_month_id', 'selectRouteName':'establishment_select_date'} %}
{% endblock %}

{% block submenu %}
	{% render "NamecoUserSchedulerBundle:SubMenu:establishmentMonth" with {'year': year, 'month': month} %}
{% endblock %}

{% block calendar %}
    {{ parent() }}
    <span id="schedule_new_prefix" style="display:none;">{{ path('schedule_new_establishment_prefix', {'userId': userId, 'establishmentId': id }) }}</span>
{% endblock %}